//网页头部脚部共享 子页替换导航class
function replaceNavClass(index){
    var aLia = $('.nav li a');
    var aLiI = $('.nav li .solid');
    aLia.attr('class','');
    aLia.eq(index).addClass('a-active');
    aLiI.attr('class','solid');
    aLiI.eq(index).addClass('solid-active');
}